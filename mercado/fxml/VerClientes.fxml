<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<StackPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:id="rootVC"
           prefHeight="400.0" prefWidth="600.0" fx:controller="mercado.controllers.VerClientes">
    <BorderPane prefHeight="400.0" prefWidth="600.0" fx:id="root">
        <top>
            <HBox alignment="CENTER">
                <children>
                    <Label text="CLIENTES" fx:id="titulo"/>
                </children>
            </HBox>
        </top>
        <center>
            <TableView fx:id="tabela"/>
        </center>
    </BorderPane>
</StackPane>
